let age = 28; 
if (age < 5) {
    console.log("The user is a Toddler.");
} else if (age >= 5 && age <= 12) {
    console.log("The user is a Child.");
} else if (age >= 13 && age <= 19) {
    console.log("The user is a Teenager.");
} else if (age >= 20 && age <= 35) {
    console.log("The user is a Young Adult.");
} else if (age >= 36 && age <= 60) {
    console.log("The user is Middle-Aged.");
} else {
    console.log("The user is a Senior.");
}
